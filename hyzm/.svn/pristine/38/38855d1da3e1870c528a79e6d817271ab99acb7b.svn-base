.detail_visa_tips{margin-top:-1px;padding:12px 0;background:#F0F9FF;border:1px solid #D4E5F2;}
.detail_visa_tips a{float:left;margin-left:-1px;padding:2px 20px;white-space:nowrap;border-left:1px solid #D4E5F2;display:inline;line-height:13px;}
.detail_visa_tips .current{color:#333;font-weight:bold;}
.detail_visa_content{margin-top:-1px;padding:15px 20px 0;border:1px solid #D4E5F2;}
.visa_type a{float:left;padding:2px 10px;margin-right:10px;border-radius: 5px;}
.visa_type a:hover,.visa_type .current{background:#6bc2fa;color:#fff;text-decoration:none;}
.visa_list{table-layout:fixed;margin-top:15px;line-height:22px;}
.visa_list th,.visa_list td{padding:10px 0;text-align:left;color:#666;border-top:1px dotted #ccc;vertical-align:top}
.visa_list th{font-weight:bold;width:150px;color:#333;}
/*======= 用户点评 =======*/
.pkg_user_comments_t {margin-top:-5px;color:#666;}
.pkg_user_comments_t span {font:bold 30px/1.5 Arial, simsun, sans-serif; color:#0060be; padding-right:0.2em; }
.pkg_user_comments_m {margin-bottom:10px;color:#333; }
.pkg_user_comments_m li {padding:16px 0 8px 0; border-bottom:1px dotted #ccc;}
.pkg_user_comments_m h4 { position:relative; font:bold 12px/1.5 Arial, simsun, sans-serif; color:#333; }
.pkg_user_comments_m h4 .seo_link{color:#333;}
.pkg_user_comments_m .pkg_user_time { font-size:12px; font-weight:normal; color:#999; }
.pkg_user_comments_m p { padding-top:3px; color: #666; }
.pkg-heart-1, .pkg-heart-2, .pkg-heart-3, .pkg-heart-4, .pkg-heart-5 { display:inline-block; width:75px; height:14px; background-image:url(http://pic.c-ctrip.com/vacation_v1/bg_heart.png?20131225.png); background-repeat:no-repeat; position:relative; zoom:1; vertical-align:middle; }
.pkg-heart-1:hover, .pkg-heart-2:hover, .pkg-heart-3:hover, .pkg-heart-4:hover, .pkg-heart-5:hover { text-decoration:none; padding:0px; }
.pkg-heart-1 span, .pkg-heart-2 span, .pkg-heart-3 span, .pkg-heart-4 span, .pkg-heart-5 span { display:none; position:absolute; padding:2px 4px 5px; color:#bd4f16; min-width:90px; font:12px/1.5 Arial, simsun, sans-serif; top:-28px; word-break:keep-all; white-space:nowrap; }
.pkg-heart-1 span { left:-14px; }
.pkg-heart-2 span { left:1px; }
.pkg-heart-3 span { left:16px; }
.pkg-heart-4 span { left:30px; }
.pkg-heart-5 span { left:46px; }
.pkg-heart-1:hover span, .pkg-heart-2:hover span, .pkg-heart-3:hover span, .pkg-heart-4:hover span, .pkg-heart-5:hover span { display:block; border:none; background:#fec96d url(http://pic.c-ctrip.com/packages111012/bg_heart_trangle.png) 0 100% no-repeat; }
.pkg-heart-1 { background-position:-60px 0; }/*1 heart*/
.pkg-heart-2 { background-position:-45px 0; }/*2 heart*/
.pkg-heart-3 { background-position:-30px 0; }/*3 heart*/
.pkg-heart-4 { background-position:-15px 0; }/*4 heart*/
.pkg-heart-5 { background-position:0 0; }/*5 heart*/
.pkg_user_comments_m .ctrip_feedback{color: #A57748;padding:8px;border:1px solid #FFCA63;background-color:#FFFBF3;margin-top:3px;}
.pkg_user_comments_m .ctrip_feedback span{color:#999;}
.user_left{ float: left; width: 860px;}
.user_right{ float: right; width: 60px; border: 1px solid #ff962d; text-align: center;}
.user_pro{ margin:5px; line-height: 22px;}
.user_pro .pkg_user_time{ margin-right: 20px;}
.user_right .comment_back{ display: block; height: 22px; line-height: 22px; background-color: #ff962d; color: #fff;}
.user_right .user_prise{ font-size:12px; color: #ff962d;}
.user_right .user_prise em{ font-style: normal;font-size: 20px; font-family:Arial, simsun, sans-serif; font-weight: bold; margin-right: 2px;}
/*======= 咨询问答 =======*/
.consult_nav{border-bottom:1px dotted #ccc;padding-bottom:15px;}
.visa_question_tnt li { border-bottom:#ccc dotted 1px; padding:15px 0; }
.visa_question_tnt li .seo_link{color:#333;}
.visa_question_tnt li.noborder { border-bottom:none; }
.visa_question_yellow,.visa_question_blue { padding-left:90px;line-height:20px; }
.visa_question_tnt .no_problem{padding-bottom:15px;border:0 none;}
.visa_question_yellow .icon_l, .visa_question_blue .icon_l { display:inline-block; width:70px; margin-left:-90px;vertical-align:baseline;}
.visa_question_yellow .grey { color:#999999; }
.visa_sort, .visa_content{ padding-left:75px; color:#333; line-height:20px; }
.visa_sort .title03, .visa_content .title03{margin-left:-75px; display:inline-block; width:75px;vertical-align:middle;}
.visa_sort a{display:inline-block;margin-right:10px;padding:2px 6px;vertical-align:middle;color:#0080c4;border-radius: 2px;}
.visa_content{margin-top:10px;}
.visa_content *{display:inline-block;vertical-align:middle;}
.visa_content .visa_btn01{margin:0}
.visa_content .line{padding:0 15px;color:#ccc;font-size:18px;}
.question_notice { padding-top: 10px; }
.visa_sort .select, .visa_sort a:hover {background:#67c2fc;color:#fff;text-decoration:none}
.visa_question_blue {margin-top:5px;color:#a57748;}
.visa_question_my .icon_l{color:#FD973E;}
.icon_l { padding-left: 20px;line-height: 20px; }
.icon_ask { background-position: -293px -36px; }
.icon_reply { background-position: -293px -57px; }
.detail_loading{margin:0 auto;text-align:center;font:18px microsoft yahei,simsun,sans-serif;vertical-align: middle;padding: 20px 0;} 
.detail_loading strong { font-weight: normal; }
.detail_loading img{ vertical-align: middle; margin-right: 10px;}
.btn_orange { display: inline-block;padding: 0 10px;line-height: 26px;height: 26px;background: #ffb000;color: #fff!important;}
.btn_orange:hover {background-color: #ff961b;text-decoration: none;}
.bottom_hot_recommend {margin-bottom: 20px;border: 1px solid #d5d5d5;background: #fff;box-shadow: 1px 1px 1px #ebeef0;zoom: 1;}



