@charset "utf-8";
/* CSS Document */
 *html{  
             background-image:url(about:blank);  
             background-attachment:fixed;  
         }/*此代码解决IE6.0下不会出现闪动*/
body{
	margin:0;padding:0;font-size: 12px;font-family: "宋体";
}
ul,li,h1,h2,h3,h4,dl,dt,dd,input{
	margin:0;padding:0;list-style: none;
}
.fl{
	float: left;
}
.fr{
	float: right;
}
.gray_clor{
	color: #808080;
}
.red_clor{
	color:#f00;
}
 /*卖家中心--sell_cont*/
 .sell_cont{
 	width: 1140px;
 	margin:0 auto;
 	overflow: hidden;
 }
 .sel_con_top{
 	height: 30px;
 	line-height: 30px;
 	margin-top: 20px;
	background: #F0F0F0;
	border-top: 2px solid #cc0000;
	padding-right: 15px;
 }
 .sel_con_top ul li{
	float: left;
	width: 102px;
	text-align: center;
	font-weight: 700;
	font-size: 14px;
	color: #000;
	cursor: pointer;
 }
 .sel_main{
 	margin-top: 12px;
 	overflow: hidden;
 }
.ws_se{
	border-right: 2px solid #fff;

 }

  /*左边-标题*/
