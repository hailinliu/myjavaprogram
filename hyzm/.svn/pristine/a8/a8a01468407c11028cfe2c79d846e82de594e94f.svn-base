}
.fanye_bbj{
	margin:20px auto 0;
	height:26px;
	text-align: center;
}
.fanye_bbj a{
	padding:4px 5px;
	background: #fff;
	border:1px solid #ccc;
	margin-right: 8px;
}
.sel_main_rt h1,.listWrap h1{
	height: 40px;
	line-height: 40px;
	background: #A80000;
	color: #fff;
	font-size: 14px;
	font-weight: bold;
	text-align: center;
}
.mm_tishi{
	position:fixed;  /*支持实现w3c标准的浏览器*/
   _position:absolute; /*单独针对IE6*/
	width: 218px;
	overflow: hidden;
	background:#fff;
	top:326px; /*距离顶部200px*/
    right:140px;  /*距离右边200px*/
    _top:expression(eval(document.documentElement.scrollTop+326));  /*+200是IE6.0下面距离浏览器窗口顶部的位置*/
    /*IE6.0下面距离底部位置为0px*/
    /*_top:expression(eval(document.documentElement.scrollTop+document.documentElement.clientHeight-this.offsetHeight-(parseInt(this.currentStyle.marginTop,10)||0)-(parseInt(this.currentStyle.marginBottom,10)||0)));*/
}

.mm_tishi h4{
	height: 45px;
	line-height: 52px;
	background-color: #e8504f;
	border-top-left-radius: 8px;
	border-top-right-radius: 8px;
	color: #fff;
	text-align: center;
	font-size: 18px;
	font-weight: bold;
}
.mm_tishi h4 .close-btn{
	display: block;
	position: absolute;
	top: 16px;
	right: 20px;
	padding: 3px;
	text-decoration: none;
	background: url(../../images/baibaojie/kf_bg.png) no-repeat;
	background-position: -74px -124px;
	width: 12px;
	height: 12px;
}
.mm_tishi h4 i{
	position: absolute;
	display: block;
	top: 16px;
	left: 16px;
	width: 23px;
	height: 18px;
	background: url(../../images/baibaojie/kf_bg.png) no-repeat;
	background-position: -93px -123px;
}
.shoper_rak{
	background: #EAEAEA;
	padding:6px 13px;
	overflow: hidden;
}
.shoper_rak p{
	line-height: 26px;
	text-indent: 2em;
}
.about_mor_dd{
	height: 30px;
	line-height: 30px;
	background-color: #e8504f;
	border-bottom-left-radius: 8px;
	border-bottom-right-radius: 8px;
	color: white;
	font-size: 14px;
	font-weight: bold;
	padding-left: 24px;
}
.about_mor_dd span{
	margin-right: 20px;
}
 



