 }

         /*交易信息-发货-第二步*/
.flow-steps {
	overflow: hidden;
	margin: 20px 0;
	width:1140px;
}
.flow-steps li.done {
	color: #f60;
}
.flow-steps li {
float: left;
background: #e4e4e4 url(http://a.tbcdn.cn/sys/common/img/flow_steps_bg.png) no-repeat 100% 0;
font-size: 14px;
font-weight: bold;
height: 23px;
line-height: 23px;
text-align: center;
color: #404040;
padding: 0 15px 0 0;
overflow: hidden;
}
.flow-steps li .first {
	background: #e4e4e4 url(http://a.tbcdn.cn/sys/common/img/flow_steps_bg.png) no-repeat -12px -69px;
}
.flow-steps .num5 li {
	width: 175px;
}
.flow-steps li.done .first {
background-color: #ffe6ba;
background-position: -12px -115px;
}

.flow-steps li span, .flow-steps li strong {
display: block;
}
.flow-steps li.done {
background-color: #ffe6ba;
background-position: 100% -46px;
color: #f60;
}
.flow-steps li.two_current{
	background-color: #f60;
	color:#fff;
}
.flow-steps li.current-prev {
background-position: 100% -23px!important;
}
.flow-steps li.last-current {
background-color: #ffe6ba;
color: #f60;
background-position: 100% 33px;
border-top-right-radius:4px;
border-bottom-right-radius:4px;
}
.crumbs{
	margin: 20px 0;
}

.curent_order_details{
	padding:20px;
	border:1px solid #86BCFA;
	overflow: hidden;
}
.curent_order_details h3{
	font-size: 14px;
	font-weight: 700;
}
.od_box_bt{
	margin-top: 20px;
}
.od_box_bt input,.fahuo_btn input{
	padding:4px 7px;
