	margin-bottom: 10px;
}
.od-tim{
	overflow: hidden;
}
.od-tim li{
	float: left;
}
.order-detail{
	border:1px solid #AEC7E5;
	padding:20px;
	overflow: hidden;
}
.order-detail li{
	height: 28px;
	line-height: 28px;
}
.order-bt span{
	display: block;
	float: left;
	text-align: center;
}
.mar01{
	width: 410px;
}
.mar02{
	width: 60px;
}
.mar03,.mar04{
	width: 115px;
}
.order-ric{
	border-bottom: 1px solid #DDDDDD;
	height: 40px;
	line-height: 40px;
}
                /*发货页面*/
.flow-steps li.three_flow{
	background: #f60;
	color: #fff;
}
.ser_txt{
