/*选项卡【活动介绍】*/
.z_mod_tit{
    position: relative;
    z-index:100;
}
.z_mod_tit .slide_list{
    position: absolute;
    left:155px;
    bottom:0;
    z-index:100;
}
.z_mod_tit .slide_list li{
    float:left;
    width:70px;
    text-align: center;
    line-height: 40px;
    margin-right: 30px;
}
.z_mod_tit .slide_list li a{
    width:100%;
    height:100%;
    font-size:14px;
    display: block;
    color:#5e5e5e;
    font-family: "Microsoft Yahei";
}
.z_mod_tit .slide_list li.current a{
    color:#ea544a;
}
.z_mod_tit .slide_list li.current {
    border-bottom:2px solid #ea544a;
}

.z_mod_tit .slide_list li a:hover{
    color:#ea544a;
}

/*【藏品列表】*/
.cp_list{
    position: relative;
}
.cp_h_tit {
    overflow: hidden;
}

/*排序*/
.cp_sort{
    position: absolute;
    top: 40px;
    right: 30px
}
.cp_sort span{
    font-size: 16px;

    display: inline-block;
    width: 85px;
    height: 37px;
    line-height: 35px;

}
.cp_sort span.sort_text{
    color: #333;
    text-align: center;
    font-weight: bold;
}
.cp_sort span.sort_s{
     color: #fff;
     text-indent: 15px;
     letter-spacing:2px;
     font-size: 20px;
     background: url(../../images/gotohyzm/sortbg0.gif) no-repeat center center;
     cursor: pointer;
     margin-right: 20px;
}
.cp_sort span.sort_s:hover,.cp_sort span.h_click{
     color: #fff;
     background: url(../../images/gotohyzm/sortbg1.gif) no-repeat center center;
}


.cp_h_tit h3 {
    margin: 0 0 20px 0;
    height: 90px;
    background: url(../../images/gotohyzm/nicb.png) no-repeat center bottom;
    padding-top: 30px;
    font-size: 40px;
    text-align: center;
    font-weight: 100;
    color: #A10000;
}
.cplist_ul{
    overflow: hidden;
    zoom: 1;
}
.cplist_ul li{
    overflow: hidden;
    padding: 0px 0px 15px;
    width: 226px;
    float: left;
    margin-right: 30px;
    box-shadow: 0px 0px 40px #ccc inset;
    /* inset 向内阴影 */
    margin-top: 30px;
}

.cplist_ul li.hover,.cplist_ul li:hover{
    background: #fff;
    box-shadow: 0px 0px 40px #ccc inset;
    /* inset 向内阴影 */;
}
.cplist_ul li.mar_none{
    margin-right: 0px;
}
.cp_img a {
    display: block;
    width: 220px;
    height: 220px;
    overflow: hidden;
    border: 1px solid #e0e0e0;
    padding: 2px;
    position: relative;
}
.cp_img img {
    display: block;
    width: 220px;
    height: 220px;
}

.cp_tit{
    height:55px;
    text-align: center;
    margin-top: 10px;
    overflow: hidden;
}
.cp_tit a{
    display: block;
    height: 30px;
    line-height: 30px;
    font-size: 15px;
    overflow: hidden;
    padding: 0 10px
}
.cp_tit p{
    font-size: 14px;
    line-height: 25px;
}
.cp_toupiao{
    margin-top: 10px;
    font-size: 14px;
    padding: 0 15px;
    line-height: 40px;
    overflow: hidden;
    color: #333;
}
.cp_toupiao span i{
    color: #a10000;
    font-size: 16px;
}
.cp_toupiao input{
    display: block;
    width: 100px;
    height: 40px;
    color: #fff;
    background: url(../../images/gotohyzm/toup.gif) center center no-repeat;
    cursor: pointer;
}
.cp_toupiao input:hover,.cplist_ul li:hover input{
    background: url(../../images/gotohyzm/toup2.gif) center center no-repeat;
    cursor: pointer;
}
