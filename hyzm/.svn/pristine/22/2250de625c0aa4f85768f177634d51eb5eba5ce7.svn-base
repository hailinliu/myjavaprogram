/*中奖名单*/
.hot-topic{
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border: 1px solid #dddbdc;
    margin-bottom: 20px;
    background: #fff;
    padding: 0 0 10px;
}
.hot-topic ul {
    padding-top: 15px;
    padding-bottom: 10px;
}
.hot-topic li {
    padding: 2px 10px 2px 30px;
    display: inline-block;
    width: 220px;
    height: 32px;
    margin-top: 5px;
    margin-bottom: 5px;
    background-image: url("../../images/gotohyzm/round.gif");
    background-position: 15px center;
    background-repeat: no-repeat;
}
.yuan_d li {
    padding-left:20px;
    background-image: url("../../images/gotohyzm/round.gif");
    background-position: 15px center;
    background-repeat: no-repeat;
}
.hot-text{
    width: 150px;
    padding-left: 5px;
    line-height: 20px;
    float:left;
    overflow: hidden;
    white-space: nowrap;
    text-overflow:ellipsis;
}
.hot-text h3{
    font-size: 12px;
}
.hot-text h3 a{
    color:#474e5d;
    line-height: 32px;
    font-family: "Microsoft YaHei", Arial;
}
.hot-text h3 a:hover{
    color:#a10000;
    text-decoration: underline;
}
.hot-text p a{
    color:#a5a5a5;
}
.hot-count{
    width: 56px;
    height: 32px;
    float: right;
    background: url("../../images/gotohyzm/topic.png");
}
.hot-count a{
    width: 56px;
    height: 32px;
    display: inline-block;
    color: #fff;
    text-align: center;
    line-height: 32px;
}
.hot-count a:hover{
    color: #fff;
}

.pro-more{
    position: absolute;
    right: 20px;
    top:3px;
    font-family: "Microsoft Yahei";
}
.pro-more a{
    color: #a5a5a5;
}
.pro-more a:hover{
    color: #005ea7;
}


