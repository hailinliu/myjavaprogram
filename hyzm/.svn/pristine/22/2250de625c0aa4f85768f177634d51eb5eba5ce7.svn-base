/*【我要上华豫】*/
.jshao-box {
    width: 678px;
    height: 290px;
    overflow: hidden;
    position: relative;
    padding-left: 30px;
}
.qi_time{
    padding-left: 30px;
    color: #a10000;
}
.jshao-box .Module_c {
    position: absolute;
    border: none;
    background: #fff;
}
.jshao-box .Module_c .Module_text {
    float: left;
    margin-left: 30px;
    width: 290px;
}
.jshao-table {
    border-left: 2px solid #e6e6e6;
    height: 195px;
    position: absolute;
    right: 0;
    top: 30px;
    width: 130px;
    z-index: 5;
    padding-top: 25px;
}
.jshao-table ul li {
    border-left: 2px solid #e6e6e6;
    height: 55px;
    left: -2px;
    position: relative;
    text-indent: 15px;
}
.jshao-table ul li.current,.jshao-table ul li.ui-tabs-active, .jshao-table ul li.ui-state-active {
    border-left: 2px solid #ea544a;
}
.jshao-table ul li a{
    color: #474e5d;
    cursor: pointer;
    display: block;
    height: 30px;
    font-size: 16px;
    line-height: 30px;
    overflow: hidden;
    padding-right: 10px;
    position: relative;
    text-overflow: ellipsis;
    top: 12px;
    white-space: nowrap;
}
.jshao-table ul li.ui-tabs-active a, .jshao-table ul li.ui-state-active a {
    color: #a10000;
    font-weight: bold;
}
