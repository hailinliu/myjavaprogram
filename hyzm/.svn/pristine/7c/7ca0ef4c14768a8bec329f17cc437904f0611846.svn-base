@charset "utf-8";
/* CSS Document */
body{margin:0;padding:0;}

.ul,li,input,selet{margin:0;padding:0;list-style:none;}

.wycz_con{width:852px;padding-left:38px;}
.wycz_con li{float:left;height:27px;line-height:27px;margin-bottom:14px;}
.wycz_con input{border:1px solid #ccc;height:27px;line-height: 27px;
	padding-left:8px;font-size:16px;font-weight:bold}
.wycz_con_tit{width:80px;font-size:14px;}
.wycz_con_nr{width:772px;}
.redfont{color:#F00;}

.cz_main_box{height:560px;}

/*选择银行*/
.wy_bank{clear:both;margin-left:38px;height:258px;padding-left:80px;}
.bankList{width:150px;height:30px;border:1px solid #dbdbdb;padding: 5px 2px 5px 5px;float:left;margin:5px 5px 0 0;line-height:29px;background:#fff;}
.bankList input{margin-top:9px;float:left;}
.bankList img{cursor:pointer;float:left;margin-left:9px;}
.wy_cz_select{width:120px;height:24px;line-height:24px;cursor:pointer;border:1px solid #dbdbdb;font-size:14px;color:#333;font-weight:bold;background:#FBFADB;}
.wycz_zfb{height:54px;clear:both;background:#E1F1FE;margin-top:10px;width:329px;padding-left:5px;margin-left:117px;}

.pa_bank{margin-right:328px;margin-bottom:20px;}

/*充值按钮*/
.cz_btn{height:30px;width:128px; border-radius:2px;background:#b61519;color:#fff;cursor:pointer;font-size:14px;margin:20px 0 20px 116px;font-weight:bold;text-align:center;}
.cz_btn:hover{background:#c3393c;}

/*充值提醒*/
.tixing{color:#FF0000;margin-left:112px;line-height:26px;font-size:14px;height:100px;}


