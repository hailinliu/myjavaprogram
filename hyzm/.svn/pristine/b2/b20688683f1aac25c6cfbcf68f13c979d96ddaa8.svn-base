@charset "utf-8";
/* CSS Document */

/* 网上报名信息表 */

/*公共样式*/

.t_center{text-align: center;}
.t_bold{font-weight: bold;}
.t_red{color: #990000;}
.content2{
	background: #FFFDFD;
}
.onuser_tit{
	/*background:url(../images/youce_bg.jpg) top center no-repeat;*/
	background:#f3f3f3;
	width:852px;
	height:37px;
	line-height:37px;
	padding-left:38px;
	margin-top:0px;
	font-size:16px;
	color:#CF3404;
	font-weight:bold;
	border:1px solid #d6d1cb;
}

.onuser_tit span{
	font-size:12px;
	font-weight:lighter;
}

.onuser_cont{
	width:890px;
	height:auto;
	border:1px solid #d6d1cb;
	border-top:0 none;
	overflow: hidden;
}
.onuser_cont img{
	float:left;
	margin:14px 34px 0 37px;
}
.onuser_cont .n_info{
	float:left;
	height:87px;
	width:140px;
	overflow: hidden;
	border-right:1px dotted #bcbcbc;
	margin-right: 0px;
}
.onuser_cont .n_info span{
	display: block;
	/*width: 100px;*/
	height: 30px;
	line-height: 30px;
	margin: 30px auto;
	text-align: center;
	font-weight: bold;
	font-size: 14px;


}
/*列表内容*/
.onuser_list{
	border-left:0px dotted #bcbcbc;
	width:100%;
	height:166px;
	display:block;
	padding-top:0px;
	font-size: 12px;
}

/*左侧项目介绍*/


.onuser_list li{
	height:39px;
	line-height:39px;
	border-bottom:1px dotted #bcbcbc;
	width:100%;
	color:#333;
	padding-left: 15px;
	overflow: hidden;
	float: left;
}

.oncl3 li{
	height:29px;
	line-height:29px;

}
.onuser_list li span{
	float: left;
	display: inline-block;
	overflow: hidden;
	/*line-height: 3;*/

}
.onuser_list .lname_info{  
	font-weight: bold;
	font-size: 14px;
	width: 170px;
	height: 39px;
	display: block;
	text-align: right;
	float: left;
	padding-right: 15px;
	margin-right: 15px;
	color: #990000;
	border-right:1px dotted #bcbcbc;
	overflow: hidden;
}
.onuser_list .info2{  
	height:82px;
	padding-left: 15px;
	overflow: hidden;

}

.onuser_list li span.red{
	width:100px;
	height:39px;
	display:block;
	text-align:right;
	float:left;
	margin-right:15px;
	color:#990000;
}
.onuser_list li.none{
	border-bottom:0 none;
}
.onuser_list li span.revise{
	margin-left:36px;
}
.onuser_list li.none span.revise{
	margin-left:95px;
}
.onuser_list li span.revise a{
	color:#40a8cf;
}
.onuser_list li span.revise a:hover{
	color:#a00104;
	text-decoration:underline;
}
.onuser_list li span.revise2 a{
	color:#40a8cf;
}
.onuser_list li span.revise2 a:hover{
	color:#a00104;
	text-decoration:underline;
}
.onuser_list li.name span.revise{
	margin-left:102px;
}
.onuser_list li.add span.revise{
	margin-left:90px;
}
.onuser_list li.none span.revise2{
	margin-left:60px;
}
.onuser_wz{
	margin-top:5px;
	text-align:right;
	color:#a00104;
}

/*横排li*/
.onuser_k li{
	width: 430px;
	float: left;
}
/*input*/
.input_on{
padding:2px 8px 0pt 3px;
height: 25px;
line-height: 25px;
margin-top: 5px;
font-size: 14px;
border:1px solid #999;
background-color:#FFFFCC;
}
.input_off{
padding:2px 8px 0pt 3px;
height: 25px;
line-height: 25px;
margin-top: 5px;
font-size: 14px;
border:1px solid #CCC;
background-color:#FFF;
}
.input_move{
padding:2px 8px 0pt 3px;
height: 25px;
line-height: 25px;
margin-top: 5px;
font-size: 14px;
border:1px solid #999;
background-color:#FFFFCC;
}
.input_out{
/*height:16px;默认高度*/
padding:2px 8px 0pt 3px;
height: 25px;
line-height: 25px;
margin-top: 5px;
font-size: 14px;
border:1px solid #CCC;
background-color:#FFF;
}
/*form*/
ul.input_test{
margin:20px auto 0 auto;
width:500px;
list-style-type:none;
}
ul.input_test li{
width:500px;
height:22px;
margin-bottom:10px;
}
.input_test label{
float:left;
padding-right:10px;
width:100px;
line-height:22px;
text-align:right;
font-size:1.4em;
}
.input_test p{
float:left;
_margin-top:-1px;
}
.input_test span{
float:left;
padding-left:10px;
line-height:22px;
text-align:left;
font-size:1.2em;
color:#999;
}
.input_text{
border: 0;
border-bottom:1px solid #999;
background: none;
width:800px;

}

/*sleles*/
.sle_cs{
	width: 150px;
	height: 28px;
	margin-top: 5px;
font-size: 12px;
border:1px solid #CCC;
background-color:#FFF;

}
.none_cad1{
	display:none;
	position:absolute;

	top:0%;
	left:0%;
	width:100%;
	height:100%;
	background-color:black;
	z-index:1002;
	-moz-opacity:0.6;
	opacity:.60;
	filter:alpha(opacity=60);
}
/*弹出层 效果1*/
.black_overlay{
	display:none;
	position:absolute;
	top:0%;
	left:0%;
	width:100%;
	height:100%;
	background-color:black;
	z-index:1001;
	-moz-opacity:0.8;
	opacity:.80;
	filter:alpha(opacity=80);
}
.white_content{
	display:none;
	position:absolute;
	top:100px;
	left:50%;
	margin-left:-480px; /*注意这里是宽度的一半*/
	padding:16px;
	border:16px solid #D3D3D3;
	background-color:white;
	z-index:1002;
	overflow:auto;
	/*position:fixed;  /* 随窗口滚动*/*/

	/*_position:absolute; 
	_left: expression(documentElement.scrollLeft + documentElement.clientWidth - this.offsetWidth); 
	_top: expression(documentElement.scrollTop + documentElement.clientHeight - this.offsetHeight); 
	*/

}
.white_content a.close {
display: block;
position: absolute;
width: 20px;
height: 20px;
right: 0px;
top: 0px;
background: url("../images/xnone.gif") no-repeat 0 100%;
}
